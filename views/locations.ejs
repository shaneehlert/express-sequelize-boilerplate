<h3>Saved Locations</h3>
<% if (info) { %>
  <h3>info: <%= info.latitude +', '+ info.longitude +', ' + info.locationName%></h3>

<form method="POST" action="/savedlocation">
<input type="hidden" name="latitude" value="<%= info.latitude %>">
  <input type="hidden" name="longitude" value="<%= info.longitude %>">
  <input type="hidden" name="apiendpoint" value="<%= info.apiEndpoint %>">
  <input type="hidden" name="inittime" value="<%= info.initTime %>">
  <input type="hidden" name="locationname" value="<%= info.locationName %>">
  <input type="submit">
<% } %>
</form>